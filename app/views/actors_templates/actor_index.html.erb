<h1>List of all actors</h1>

<table border="1">
<% @list_of_actors.each do |an_actor| %>
  <tr>
  <td><%= an_actor.id %></td>
  <td>
    <img src="http://robohash.org/<%= an_actor.name %>">
  </td>
  <td><%= an_actor.name %></td>
  <td><%= an_actor.dob %></td>
  <td>
    <a href="/actors/<%= an_actor.id %>">
      Show details
  </td>
  
  
  </tr>
<% end %>
